ECS_CLUSTER=<%= node["opsworks_ecs"]["ecs_cluster_name"] %>

<% if node["opsworks_ecs"]["ecs_engine_auth_type"] -%>
ECS_ENGINE_AUTH_TYPE=<%= node["opsworks_ecs"]["ecs_engine_auth_type"] %>
